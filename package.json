{
  "name": "git-organized",
  "version": "0.0.0",
  "description": "CLI tool for cloning and organizing your git repositories in a go-like structure.",
  "main": "src/cli.js",
  "scripts": {
    "babel-node": "babel-node",
    "debug": "DEBUG=${DEBUG:-'git-organized:*'} npm run watch",
    "develop": "npm-run-all --serial lint:fix debug",
    "doc": "jsdoc -c '.jsdoc.js'",
    "start": "babel-node src/index.js",
    "eslint": "eslint",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "prestart": "npm outdated",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "watch": "nodemon --exec npm run babel-node -- src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rot26/git-organized.git"
  },
  "keywords": [
    "clone",
    "find",
    "git",
    "go-like",
    "organize",
    "recursive",
    "search"
  ],
  "author": {
    "name": "Chris Coleman",
    "email": "chris@rot-26.com",
    "url": "rot-26.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rot26/find-git-directories/issues"
  },
  "homepage": "https://github.com/rot26/find-git-directories#readme",
  "dependencies": {
    "commander": "^3.0.2",
    "find-git-repos": "^0.0.0",
    "git-url-parse": "^11.1.2",
    "node-cmd": "^3.0.0",
    "simple-git": "^1.126.0"
  },
  "devDependencies": {
    "ajv": "^6.10.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^6.4.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.3"
  }
}

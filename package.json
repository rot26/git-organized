{
  "name": "git-organized",
  "version": "0.1.0",
  "description": "CLI tool for cloning and organizing your git repositories in a go-like structure.",
  "main": "src/index.js",
  "scripts": {
    "babel-node": "babel-node",
    "debug": "DEBUG=${DEBUG:-'git-organized:*'} npm run watch",
    "develop": "npm-run-all --parallel test:watchAll debug",
    "start": "babel-node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "watch": "nodemon --exec npm run babel-node -- src/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rot26/git-organized.git"
  },
  "keywords": [
    "clone",
    "find",
    "git",
    "go-like",
    "organize",
    "recursive",
    "search"
  ],
  "author": {
    "name": "Chris Coleman",
    "email": "chris@rot-26.com",
    "url": "rot-26.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rot26/find-git-directories/issues"
  },
  "homepage": "https://github.com/rot26/find-git-directories#readme",
  "dependencies": {
    "commander": "2.17.1",
    "find-git-repos": "0.0.0",
    "git-url-parse": "10.0.1",
    "node-cmd": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.4.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "jest": "23.5.0",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "regenerator-runtime": "0.12.1"
  }
}
